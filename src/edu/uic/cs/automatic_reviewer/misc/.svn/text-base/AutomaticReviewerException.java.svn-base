package edu.uic.cs.automatic_reviewer.misc;

public class AutomaticReviewerException extends RuntimeException {
	private static final long serialVersionUID = 1L;

	public AutomaticReviewerException() {
		super();
	}

	public AutomaticReviewerException(String message, Throwable cause) {
		super(message, cause);
	}

	public AutomaticReviewerException(String message) {
		super(message);
	}

	public AutomaticReviewerException(Throwable cause) {
		super(cause);
	}

}
